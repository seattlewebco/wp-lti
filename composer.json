{
    "name": "seattlewebco/wp-lti",
    "description": "WordPress Learning Tools Interoperability Integration",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "David Jensen",
            "email": "david@seattlewebco.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/IMSGlobal/lti-1-3-php-library"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1.0"
        }
    },
    "require": {
        "imsglobal/lti-1p3-tool": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "php-stubs/wordpress-stubs": "^5.6",
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "^2.3",
        "squizlabs/php_codesniffer": "^3.5",
        "automattic/vipwpcs": "^2.2"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "phpcs": [
          "phpcs -s -p"
        ],
        "phpcs-pre-commit": [
          "phpcs -s -p -n"
        ],
        "phpcbf": [
          "phpcbf -p"
        ],
        "post-install-cmd": [
            "composer dump-autoload -o"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    }
}
